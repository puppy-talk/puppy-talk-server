# Docker Compose 환경 변수
# 이 파일을 .env로 복사하여 사용하세요

# MySQL 설정
MYSQL_ROOT_PASSWORD=root1234
MYSQL_USER=puppy_user
MYSQL_PASSWORD=puppy_pass

# 애플리케이션 설정
BUILD_DATE=2024-01-01
VCS_REF=main

# WebSocket 허용 오리진
ALLOWED_ORIGINS=http://localhost:3000,https://puppytalk.com

# 로깅 레벨
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_PUPPY_TALK=DEBUG

